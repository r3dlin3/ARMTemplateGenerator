{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetName": {
      "value": ""
    },
    "subnetName": {
      "value": ""
    },
    {{#if existingVNET}}
    "vnetResourceGroup": {
      "value": ""
    },
    {{/if}}
    {{#unless existingVNET}}
    "vnetPrefix": {
      "value": ""
    },
    {{#unless existingSubnet}}
    "subnetPrefix": {
      "value": ""
    },
    {{/unless}}
    {{/unless}}
    {{#if hasPublicIP}}
    "dnsLabelPrefix": {
      "value": "{{name}}"
    },
    "publicIPAddressName": {
        "value": ""
    },
    {{/if}}
    {{#if remoteAccess}}
    "networkSecurityGroupName": {
        "value": ""
    },
    {{/if}}
    {{#is privateIPtype "Static"}}
    "privateIP": {
        "value": ""
    },
    {{/is}}
    {{#isnot nbDataDisk 0}}
    "sizeOfEachDataDiskInGB": {
        "value": ""
    },
    {{/isnot}}
    "nicName": {
      "value": ""
    },
    "adminUsername": {
      "value": ""
    },
    "adminPassword": {
      "value": ""
    },
    {{#if isDiagnosticsEnabled}}
    "storageName": {
      "value": ""
    },
    {{#unless existingStorage}}
    "storageAccountType": {
      "value": ""
    },
    {{/unless}}
    {{/if}}
    "vmName": {
      "value": ""
    },
    {{#if hasAutoShutdown}}
    "autoShutdownDailyRecurrence": {
      "value": "1900"
    },
    "autoShutdownTimezone": {
      "value": ""
    },
    {{/if}}
    {{#if isADJoined}}
    "domainJoinUserName": {
      "value": ""
    },
    "domainJoinUserPassword": {
      "value": ""
    },
    "domainToJoin": {
      "value": ""
    },
    "ouPath": {
      "value": ""
    },
    {{/if}}
    "vmSize": {
      "value": ""
    },
    "location": {
      "value": ""
    },
    "tagValues": {
      "value": {}
    }
  }
}