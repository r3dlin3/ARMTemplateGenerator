{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetName": {
      "value": ""
    },
    "subnetName": {
      "value": ""
    },
    {{#unless existingVNET}}
    "vnetPrefix": {
      "value": ""
    },
    {{#unless existingSubnet}}
    "subnetPrefix": {
      "value": ""
    },
    {{/unless}}
    {{/unless}}
    {{#if hasPublicIP}}
    "dnsLabelPrefix": {
      "value": "{{name}}"
    },
    "publicIPAddressName": {
        "value": ""
    },
    {{/if}}
    {{#is privateIPtype "Static"}}
    "privateIP": {
        "value": ""
    },
    {{/is}}
    {{#isnot nbDataDisk 0}}
    "sizeOfEachDataDiskInGB": {
        "value": ""
    },
    {{/isnot}}
    "nicName": {
      "value": ""
    },
    "adminUsername": {
      "value": ""
    },
    "adminPassword": {
      "value": ""
    },
    "vmName": {
        "value": ""
    },
    "vmSize": {
        "value": ""
    },
    "location": {
      "value": ""
    }
  }
}