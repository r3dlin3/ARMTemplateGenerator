{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetName": {
      "value": ""
    },
    "subnetName": {
      "value": ""
    },
    {{#if existingVNET}}
    "vnetResourceGroup": {
      "value": ""
    },
    {{/if}}
    {{#unless existingVNET}}
    "vnetPrefix": {
      "value": ""
    },
    {{#unless existingSubnet}}
    "subnetPrefix": {
      "value": ""
    },
    {{/unless}}
    {{/unless}}
    {{#if hasPublicIP}}
    "dnsLabelPrefix": {
      "value": "{{name}}"
    },
    "publicIPAddressName": {
        "value": ""
    },
    {{/if}}
    {{#if remoteAccess}}
    "networkSecurityGroupName": {
        "value": ""
    },
    {{/if}}
    {{#is privateIPtype "Static"}}
    "privateIP": {
        "value": ""
    },
    {{/is}}
    {{#is osPublisher 'CustomImage'}}
    "imageName": {
      "value": ""
    },
    "imageNameResourceGroupName": {
      "value": ""
    },
    {{/is}}
    {{#is nbDataDisk 0}}
    "diskName": {
      "value": ""
    },
    {{/is}}
    {{#isnot nbDataDisk 0}}
    "diskNames": {
      "value": ["", {{#repeat count=nbDataDisk}}""{{#unless @last}},{{/unless}}{{/repeat}}]
    },
    "sizeOfEachDataDiskInGB": {
        "value": ""
    },
    {{/isnot}}
    "nicName": {
      "value": ""
    },
    "adminUsername": {
      "value": ""
    },
    "adminPassword": {
      "value": ""
    },
    {{#if isDiagnosticsEnabled}}
    "storageName": {
      "value": ""
    },
    {{#if existingStorage}}
    "storageAccountResourceGroup": {
        "value": ""
    }
    {{/if}}
    {{#unless existingStorage}}
    "storageAccountType": {
      "value": ""
    },
    {{/unless}}
    {{/if}}
    "vmName": {
      "value": "{{name}}"
    },
    {{#if hasAutoShutdown}}
    "autoShutdownDailyRecurrence": {
      "value": "1900"
    },
    "autoShutdownTimezone": {
      "value": ""
    },
    {{/if}}
    {{#if isADJoined}}
    "domainJoinUserName": {
      "value": ""
    },
    "domainJoinUserPassword": {
      "value": ""
    },
    "domainToJoin": {
      "value": ""
    },
    "ouPath": {
      "value": ""
    },
    {{/if}}
    {{#if backup}}
    "backupVaultName": {
      "value": ""
    },
    "backupVaultResourceGroup": {
      "value": ""
    },
    "backupPolicyName": {
      "value": ""
    },
    "_artifactsLocation": {
      "value": ""
    },
    "_artifactsLocationSasToken": {
      "value": ""
    },
    {{/if}}
    {{#each extensions}}
    {{#is extensionType "Generic"}}
    {{/is}}
    {{#is extensionType "CustomScript"}}
    "scriptURL": {
      "value": ""
    },
    {{/is}}
    {{#is extensionType "DSC"}}
    {{#if onboardAzureAutomation}}
    "automationAccountName": {
      "value": ""
    },
    "automationResourceGroupName": {
      "value": ""
    },
    "nodeConfigurationName": {
      "value": ""
    },
    {{/if}}
    {{/is}}
    {{/each}}
    "vmSize": {
      "value": ""
    },
    "tagValues": {
      "value": {}
    }
  }
}