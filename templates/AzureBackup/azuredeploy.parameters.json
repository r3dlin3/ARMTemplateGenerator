      {{#is backupPolicy "Weekly"}}
      "scheduleRunDays": {
        "value": "## TO BE DEFINED ##"
      },
      "scheduleRunTimes": {
        "value": "## TO BE DEFINED ##"
      },
      "timeZone": {
          "value": "UTC"
      },
      "weeklyRetentionDurationCount": {
          "value": "## TO BE DEFINED ##"
      },
      "daysOfTheWeekForMontlyRetention": {
          "value": "## TO BE DEFINED ##"
      },
      "weeksOfTheMonthForMonthlyRetention": {
          "value": "## TO BE DEFINED ##"
      },
      "monthlyRetentionDurationCount": {
          "value": "## TO BE DEFINED ##"
      },
      "monthsOfYear": {
          "value": "## TO BE DEFINED ##"
      },
      "daysOfTheWeekForYearlyRetention": {
          "value": "## TO BE DEFINED ##"
      },
      "weeksOfTheMonthForYearlyRetention": {
          "value": "## TO BE DEFINED ##"
      },
      "yearlyRetentionDurationCount": {
          "value": "## TO BE DEFINED ##"
      },
      {{/is}}
      {{#is backupPolicy "Daily"}}
      "scheduleRunTimes": {
          "value": "## TO BE DEFINED ##"
      },
      "timeZone": {
          "value": "UTC"
      },
      "instantRpRetentionRangeInDays": {
          "value": 2
      },
      "dailyRetentionDurationCount": {
          "value": "## TO BE DEFINED ##"
      },
      "daysOfTheWeek": {
          "value": "## TO BE DEFINED ##"
      },
      "weeklyRetentionDurationCount": {
          "value": "## TO BE DEFINED ##"
