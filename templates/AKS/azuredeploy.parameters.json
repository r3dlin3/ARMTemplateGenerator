{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "aksName": {
            "value": "{{name}}"
        },
        "dnsPrefix": {
            "value": ""
        },
        "osDiskSizeGB": {
            "value": 0
        },
        "agentCount": {
            "value": 3
        },
        "agentVMSize": {
            "value": "Standard_DS2_v2"
        },
        "maxPods": {
            "value": 30
        },
        {{#if createCredentials}}
        "linuxAdminUsername": {
            "value": "azureuser"
        },
        "sshRSAPublicKey": {
            "value": "## TO BE DEFINED ##"
        },
        {{/if}}
        {{#if existingVNET}}
        "servicePrincipalObjectId": {
            "value": "## TO BE DEFINED ##"
        },
        {{/if}}
        "servicePrincipalClientId": {
            "value": "## TO BE DEFINED ##"
        },
        "servicePrincipalClientSecret": {
            "reference": {
                "keyVault": {
                    "id": "<specify Resource ID of the Key Vault you are using>"
                },
                "secretName": "<specify name of the secret in the Key Vault to get the service principal password from>"
            }
        },
        "osType": {
            "value": "Linux"
        },
        "kubernetesVersion": {
            "value": "1.14.6"
        },
        "enableHttpApplicationRouting": {
            "value": false
        },
        "enableRBAC": {
            "value": true
        },
        {{#if existingVNET}}
        "vnetName": {
            "value": "## TO BE DEFINED ##"
        },
        "vnetResourceGroup": {
            "value": "## TO BE DEFINED ##"
        },
        "subnetName": {
            "value": "## TO BE DEFINED ##"
        },
        "networkPlugin": {
            "value": "azure"
        },
        "serviceCidr": {
            "value": "10.0.0.0/16"
        },
        "dnsServiceIP": {
            "value": "10.0.0.10"
        },
        "dockerBridgeCidr": {
            "value": "172.17.0.1/16"
        },
        {{/if}}
        {{#if aadSupport}}
        "AADClientAppID": {
            "value": "## TO BE DEFINED ##"
        },
        "AADServerAppID": {
            "value": "## TO BE DEFINED ##"
        },
        "AADServerAppSecret": {
            "value": "## TO BE DEFINED ##"
        },
        "AADTenantID": {
            "value": "[subscription().tenantId]"
        },
        {{/if}}
        {{#if enableLogging}}
        "workspaceName": {
            "value": "## TO BE DEFINED ##"
        },
        {{#unless createWorkspace}}
        "workspaceResourceGroup": {
            "value": "## TO BE DEFINED ##"
        },
        {{/unless}}
        {{/if}}
        {{#if useAcr}}
        {{#unless existingVNET}}
        "servicePrincipalObjectId": {
            "value": "## TO BE DEFINED ##"
        },
        {{/unless}}
        "acrName": {
            "value": "## TO BE DEFINED ##"
        },
        {{#if createAcr}}
        "acrAdminUserEnabled": {
            "value": false
        },
        {{#unless enableGeoReplication}}
        "acrSku": {
            "value": "Basic"
        },
        {{/unless}}
        {{#if enableGeoReplication}}
        "acrReplicaLocation": {
            "value": "## TO BE DEFINED ##"
        },
        {{/if}}{{else}}
        "acrResourceGroup": {
            "value": "## TO BE DEFINED ##"
        },
        {{/if}}{{/if}}{{!-- End of useAcr--}}
        "tagValues": {
            "value": {}
        }
    }
}