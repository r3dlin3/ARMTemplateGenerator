{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "siteName": {
      "value": "{{name}}"
    },
    "appServicePlanName": {
      "value": "{{name}}-plan"
    },
    {{#unless existingASP}}
    "skuName": {
      "value": "F1"
    },
    "capacity": {
      "value": 1
    },
    {{#if isLinux}}
    "linuxFxVersion": {
      {{#if docker}}
      "value": "DOCKER|phpoidc/phpoidc:phpop"
      {{else}}
      "value": "PHP|7.4"
      {{/if}}
    },
    "startupCommand": {
      "value": ""
    },{{#if docker}}
    "dockerRegistryUrl": {
      "value": "https://index.docker.io"
    },
    "dockerRegistryUsername": {
      "value": ""
    },
    "dockerRegistryPassword": {
      "value": ""
    },{{/if}}
    {{/if}}{{!-- isLinux --}}
    {{/unless}}{{!-- existingASP --}}
    {{#if createSlot}}
    "environments": {
      "value": ["Dev","QA","UAT","Preview"]
    },
    {{/if}}
    {{#if createAppInsights}}
    "appInsightsName": {
      "value": ""
    },
    {{/if}}
    {{#if createCDN}}
    "CDNProfileName": {
      "value": ""
    },
    "CDNEndpointName": {
      "value": ""
    },
    "CDNSku": {
      "value": ""
    },
    {{/if}}
    {{#if createCache}}
    "cacheName": {
      "value": "",
    },
    "cacheSKUName": {
      "value": "Basic",
    },
    "cacheSKUFamily": {
      "value": "C"
    },
    "cacheSKUCapacity": {
      "value": 0,
    },
    {{/if}}
    "tags": {
      "value": {}
    }
  }
}